<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:focusableInTouchMode="true">

    <RelativeLayout
        android:id="@+id/rtv_hdr"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:id="@+id/btn_bck"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dp"
            app:srcCompat="@drawable/ic_back_btn" />

        <TextView
            android:id="@+id/order_detail"
            style="@style/contentTextStyle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@id/txt_execution_Details"
            android:layout_toRightOf="@id/btn_bck"
            android:gravity="center"
            android:padding="5dp"
            android:text="@string/OrderDetail"
            android:textAllCaps="true"
            android:textColor="#5173B2"
            app:fontFamily="@font/helvetica_bold" />

        <ImageView
            android:id="@+id/txt_execution_Details"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginHorizontal="5dp"
            android:src="@drawable/invoice_button_image"
            android:visibility="gone" />

        <ImageView
            android:id="@+id/img_return"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/txt_execution_Details"
            android:src="@drawable/return_order"
            android:visibility="gone" />
    </RelativeLayout>
    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
       android:gravity="center"
        android:id="@+id/rv_dailandorder"
        android:layout_below="@+id/rtv_hdr"
        android:orientation="horizontal"
        android:weightSum="1"
        android:visibility="visible">
        <!--                android:visibility="gone"-->



        <TextView
            android:id="@+id/tv_ClickHereForDialNumberAndProceedForOrderEntry"

            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dimen_10dp"
           android:layout_weight="0.8"
            android:text="Click here to call store."
            android:paddingBottom="1dp"
           android:layout_gravity="center|center_horizontal"
                android:textSize="@dimen/dimen_10dp"
            android:textColor="@color/primaryColorDark"
            />

        <ImageView
            android:id="@+id/icon"
android:layout_weight="0.2"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_gravity="center|center_horizontal"

            android:src="@drawable/makephonecall" />


    </LinearLayout>
    <LinearLayout
        android:id="@+id/ll_ctgry"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/rv_dailandorder"
        android:background="#8DC100"
        android:orientation="horizontal"
        android:paddingHorizontal="8dp"
        android:paddingVertical="5dp"
        android:weightSum="10">

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="9">

            <EditText
                android:id="@+id/ed_search"
                style="@style/editTextStyle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/search_background"
                android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890 -,()/*."
                android:hint="SEARCH"
                android:paddingHorizontal="@dimen/dimen_10dp"
                android:paddingVertical="@dimen/dimen_10dp" />

            <ImageView
                android:layout_width="30dp"
                android:layout_height="wrap_content"
                android:layout_alignTop="@id/ed_search"
                android:layout_alignBottom="@id/ed_search"
                android:layout_alignParentRight="true"
                android:layout_marginBottom="2dp"
                android:background="@drawable/b"
                android:padding="8dp"
                app:srcCompat="@drawable/ic_search" />
        </RelativeLayout>

        <ImageView
            android:id="@+id/img_ctgry"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="5dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/category_order_entry" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/llBtn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/ll_ctgry"
        android:orientation="horizontal"
        android:padding="5dp"
        android:weightSum="3">

        <LinearLayout
            android:id="@+id/llOrdHistory"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/button_bg_round"
            android:gravity="center"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_margin="3dp"
                app:srcCompat="@drawable/invoice_button_selected" />

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="#014681" />

            <TextView
                style="@style/contentTextStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:paddingHorizontal="8dp"
                android:paddingVertical="4dp"
                android:text="Store History"
                android:textColor="#014681"
                android:textSize="11sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/llSchemeAppl"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginHorizontal="5dp"
            android:layout_weight="1"
            android:background="@drawable/button_bg_round"
            android:gravity="center"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_margin="3dp"
                app:srcCompat="@drawable/ic_scheme" />

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="#014681" />

            <TextView
                style="@style/contentTextStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:paddingHorizontal="8dp"
                android:paddingVertical="4dp"
                android:text="Scheme Applicable"
                android:textColor="#014681"
                android:textSize="11sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/llPrioritySKU"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/button_bg_round"
            android:gravity="center"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_margin="3dp"
                app:srcCompat="@drawable/ic_buying" />

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="#014681" />

            <TextView
                style="@style/contentTextStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:paddingHorizontal="8dp"
                android:paddingVertical="4dp"
                android:text="Priority SKU"
                android:textColor="#014681"
                android:textSize="11sp" />
        </LinearLayout>
    </LinearLayout>

    <RelativeLayout
        android:id="@+id/ll_schm_applcbl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/llBtn"
        android:background="#ffffff"
        android:gravity="center"
        android:orientation="horizontal"
        android:paddingLeft="16dp"
        android:paddingTop="1dp"
        android:paddingRight="16dp"
        android:paddingBottom="1dp"
        android:visibility="gone">

        <TextView
            android:id="@+id/textView1_scheme"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/prodlstSchmeApplicable"
            android:textColor="#000000"
            android:textSize="13sp" />

        <TextView
            android:id="@+id/txtVw_schemeApld"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="16dp"
            android:layout_toRightOf="@+id/textView1_scheme"
            android:gravity="center|right"
            android:text=""
            android:textSize="15sp" />

    </RelativeLayout>

    <LinearLayout
        android:id="@+id/ll_header"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/ll_schm_applcbl"
        android:background="#9E9E9E"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingLeft="5dp"
        android:paddingTop="1dp"
        android:paddingRight="5dp"
        android:paddingBottom="1dp"
        android:visibility="gone"
        android:weightSum="8">

        <!-- <TextView
             android:layout_width="0dp"
             android:layout_height="wrap_content"
             android:layout_weight="1"
             android:text="@string/TodayStock"
             android:gravity="center"
             android:textSize="10sp"
             android:visibility="invisible"
             android:textStyle="bold"
             android:textColor="#201359"/>-->
        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="SKU Name"
            android:textColor="#201359"
            android:textSize="11sp"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/txt_Lststock"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="@string/stockAsOn"
            android:textColor="#201359"
            android:textSize="11sp"
            android:textStyle="bold"
            android:visibility="gone" />

        <TextView
            android:id="@+id/txt_StoreSale"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="@string/StoreSale"
            android:textColor="#201359"
            android:textSize="11sp"
            android:textStyle="bold"
            android:visibility="gone" />

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="@string/SmplQty"
            android:textColor="#201359"
            android:textSize="11sp"
            android:textStyle="bold"
            android:visibility="gone" />

        <TextView
            android:id="@+id/lbl_InvOrderHeader"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="2.5"
            android:gravity="center"
            android:text="@string/OQty"
            android:textColor="#201359"
            android:textSize="14sp"
            android:textStyle="bold" />

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1.5"
            android:orientation="vertical"
            android:weightSum="2">

            <TextView
                android:id="@+id/textView"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@string/MinDQ"
                android:textColor="#000000"
                android:textSize="11sp"
                android:textStyle="bold"
                android:visibility="gone" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="0"
                android:gravity="center"
                android:text="@string/MRP"
                android:textColor="#000000"
                android:textSize="13sp"
                android:textStyle="bold"
                android:visibility="invisible" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="2"
                android:gravity="center"
                android:text="@string/Rate"
                android:textColor="#000000"
                android:textSize="13sp"
                android:textStyle="bold" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1.5"
            android:orientation="vertical"
            android:weightSum="2">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@string/FQty"
                android:textColor="#D42625"
                android:textSize="13sp"
                android:textStyle="bold" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@string/DVal"
                android:textColor="#D42625"
                android:textSize="13sp"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1.5"
            android:orientation="vertical"
            android:weightSum="2">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@string/LODQty"
                android:textColor="#000000"
                android:textSize="13sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tv_EntryInvValHeader"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@string/OVal"
                android:textColor="#D42625"
                android:textSize="13sp"
                android:textStyle="bold" />
        </LinearLayout>

    </LinearLayout>

    <RelativeLayout
        android:id="@+id/rlTCOrderMain"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/ll_schm_applcbl"
        android:padding="2dp">

        <TextView
            android:id="@+id/tv1"
            style="@style/titleTextStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Store TARS Orders"
            android:textColor="#446EC0" />

        <ImageView
            android:id="@+id/iv1"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_alignParentRight="true"
            app:srcCompat="@drawable/ic_up" />

        <LinearLayout
            android:id="@+id/ll2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/iv1"
            android:orientation="horizontal"
            android:weightSum="2">

            <TextView
                style="@style/titleTextStyle"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1.2"
                android:background="@drawable/table_design_row"
                android:gravity="center"
                android:padding="2dp"
                android:text="Call Date"
                android:textSize="11sp" />

            <TextView
                style="@style/titleTextStyle"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.8"
                android:background="@drawable/table_design_row"
                android:fontFamily="@font/helvetica_bold"
                android:gravity="center"
                android:padding="2dp"
                android:text="Order Qty (Case) / Value"
                android:textSize="11sp" />

        </LinearLayout>

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rvStoreVisitHistory"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/ll2"
            android:layout_marginBottom="3dp" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rlCartons"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/rlTCOrderMain"
        android:padding="2dp">

        <TextView
            style="@style/titleTextStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Cartons"
            android:textColor="#446EC0" />

        <ImageView
            android:id="@+id/iv2"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_alignParentRight="true"
            app:srcCompat="@drawable/ic_up" />

        <LinearLayout
            android:id="@+id/ll3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/iv2"
            android:orientation="horizontal"
            android:weightSum="4.5">

            <TextView
                style="@style/titleTextStyle"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1.5"
                android:background="@drawable/table_design_row"
                android:fontFamily="@font/helvetica_bold"
                android:gravity="center"
                android:padding="2dp"
                android:text="Product"
                android:textSize="11sp" />

            <TextView
                style="@style/titleTextStyle"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.8"
                android:background="@drawable/table_design_row"
                android:fontFamily="@font/helvetica_bold"
                android:gravity="center"
                android:padding="2dp"
                android:text="No. of Cases"
                android:textSize="11sp" />

            <TextView
                style="@style/titleTextStyle"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.8"
                android:background="@drawable/table_design_row"
                android:fontFamily="@font/helvetica_bold"
                android:gravity="center"
                android:padding="2dp"
                android:text="Free Qty."
                android:textSize="11sp" />

            <TextView
                style="@style/titleTextStyle"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.8"
                android:background="@drawable/table_design_row"
                android:fontFamily="@font/helvetica_bold"
                android:gravity="center"
                android:padding="2dp"
                android:text="Std. Line Values"
                android:textSize="11sp"
                android:visibility="gone" />

            <TextView
                style="@style/titleTextStyle"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.9"
                android:background="@drawable/table_design_row"
                android:fontFamily="@font/helvetica_bold"
                android:gravity="center"
                android:padding="2dp"
                android:text="Net Line Value"
                android:textSize="11sp" />

            <TextView
                style="@style/titleTextStyle"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:background="@drawable/table_design_row"
                android:fontFamily="@font/helvetica_bold"
                android:gravity="center"
                android:padding="2dp"
                android:text="Edit"
                android:textSize="11sp" />

        </LinearLayout>

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rvCartonData"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/ll3"
            android:layout_marginBottom="3dp" />

    </RelativeLayout>

    <LinearLayout
        android:id="@+id/ll_rcylContainer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@id/ll_buttonsNew"
        android:layout_below="@id/rlCartons"
        android:orientation="vertical">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rv_prdct_detal"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginTop="1dp"
            android:layout_weight="0.92"
            android:scrollbars="vertical" />

        <View
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="@color/colorPrimaryDark" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#FFFFFF"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/ll_GiveOverAllDiscountSection"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingHorizontal="5dp"
                android:paddingVertical="1dp"
                android:weightSum="2">

                <TextView
                    style="@style/titleTextStyle"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Overall Discount"
                    android:textSize="12sp" />

                <TextView
                    style="@style/titleTextStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="5dp"
                    android:text="\u20b9"
                    android:textSize="12sp" />

                <EditText
                    android:id="@+id/et_Disc"
                    style="@style/editTextStyle"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:background="@drawable/et_boundary"
                    android:gravity="center"
                    android:inputType="number"
                    android:textSize="10sp" />

                <TextView
                    android:id="@+id/tvDisc"
                    style="@style/contentTextStyle"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginHorizontal="3dp"
                    android:layout_weight="0.5"
                    android:background="@drawable/button_bg_round_4"
                    android:gravity="center"
                    android:padding="5dp"
                    android:text="Apply"
                    android:textColor="@color/white"
                    android:textSize="10sp" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_marginVertical="2dp"
                android:background="#80B0BEC5" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingHorizontal="5dp"
                android:paddingVertical="1dp">

                <TextView
                    style="@style/titleTextStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Product Level Discount"
                    android:textSize="12sp" />

                <TextView
                    style="@style/titleTextStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toLeftOf="@id/tvProductLevelDiscount"
                    android:text="\u20b9"
                    android:textSize="12sp" />

                <TextView
                    style="@style/titleTextStyle"
                    android:id="@+id/tvProductLevelDiscount"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:gravity="center"
                    android:paddingHorizontal="5dp"
                    android:text="0.00"
                    android:textColor="#000000"
                    android:textSize="12sp"
                    android:textStyle="bold" />

            </RelativeLayout>

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:shrinkColumns="*"
                android:stretchColumns="*">

                <TableRow
                    android:id="@+id/tableRow1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#f1f1f1"
                    android:gravity="center_vertical"
                    android:visibility="gone"
                    android:weightSum="8">

                    <TextView
                        android:id="@+id/txt_detalis"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="4"
                        android:paddingLeft="16dp"
                        android:paddingTop="1dp"
                        android:paddingBottom="1dp"
                        android:text="@string/OrderTotal"
                        android:textColor="#201359"
                        android:textSize="14sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/txt_cr_amnt"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:padding="1dp"
                        android:text="@string/CrAmt"
                        android:textColor="#201359"
                        android:textSize="14sp"
                        android:textStyle="bold"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/txt_cr_adj"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:padding="1dp"
                        android:text="@string/CrAdj"
                        android:textColor="#201359"
                        android:textSize="14sp"
                        android:textStyle="bold"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/txt_cr_bal"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:padding="1dp"
                        android:text="@string/CrBal"
                        android:textColor="#201359"
                        android:textSize="14sp"
                        android:textStyle="bold"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/txt_RefreshOdrTot"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="3"
                        android:background="@drawable/custom_button"
                        android:gravity="center"
                        android:padding="1dp"
                        android:paddingRight="10dp"
                        android:text="@string/UpdateTotal"
                        android:textColor="#ffffff"
                        android:textSize="14sp"
                        android:textStyle="bold" />

                </TableRow>
            </TableLayout>

            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:scrollbarStyle="outsideOverlay">

                <LinearLayout
                    android:id="@+id/ll_scheme_detail"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:visibility="visible">

                </LinearLayout>
            </ScrollView>
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_buttonsNew"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="#f1f1f1"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/ll_ContactUpdateSection"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="8dp"
            android:orientation="vertical"
            android:paddingBottom="@dimen/dimen_10dp"
            android:visibility="gone">


        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_buttons"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#f1f1f1"
            android:gravity="center"
            android:orientation="horizontal"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:paddingBottom="2dp"
            android:weightSum="3">

            <Button
                android:id="@+id/btn_Cancel"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"

                android:background="@drawable/custom_button"
                android:gravity="center"
                android:text="@string/Cancel"
                android:textColor="#ffffff"
                android:textSize="14sp"
                android:visibility="gone" />

            <Button
                android:id="@+id/btn_save"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="2dp"
                android:layout_weight="1"
                android:background="@drawable/custom_button"
                android:gravity="center"
                android:text="@string/Save"
                android:textColor="#ffffff"
                android:textSize="13sp"
                android:visibility="gone" />

            <Button
                android:id="@+id/btn_saveExit"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="2dp"
                android:layout_weight="1"
                android:background="@drawable/custom_button"

                android:gravity="center"
                android:text="@string/SaveExit"
                android:textColor="#ffffff"
                android:textSize="13sp"
                android:visibility="gone" />

            <Button
                android:id="@+id/btn_sbmt"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="2dp"

                android:layout_weight="1"

                android:background="@drawable/custom_button"
                android:gravity="center"
                android:text="@string/Submit"
                android:textColor="#ffffff"
                android:textSize="14sp"
                android:visibility="gone" />


            <Button
                android:id="@+id/btn_ShowSchemeApplicableProducts"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="2dp"
                android:layout_weight="1"
                android:background="@drawable/custom_button"

                android:gravity="center"
                android:text="Scheme Products"
                android:textColor="#ffffff"
                android:textSize="13sp"
                android:visibility="visible" />

            <Button
                android:id="@+id/btn_NoOrder"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="2dp"
                android:layout_marginBottom="2dp"
                android:layout_weight="1"

                android:background="@drawable/custom_button"
                android:gravity="center"
                android:text="No Order"
                android:textColor="#ffffff"
                android:textSize="14sp" />

            <Button
                android:id="@+id/btn_ShowAllProducts"
                style="@style/contentTextStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="2dp"
                android:layout_weight="1"

                android:background="@drawable/button_bg_round_2"
                android:gravity="center"
                android:text="All Products"
                android:textColor="#ffffff"
                android:textSize="13sp"
                android:visibility="gone" />


            <Button
                android:id="@+id/btn_InvoiceReview"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="2dp"

                android:layout_weight="1"

                android:background="@drawable/custom_button"
                android:gravity="center"
                android:text="@string/InvoiceReview"
                android:textColor="#ffffff"
                android:textSize="13sp" />

            <Button
                android:id="@+id/btn_OrderReview"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="2dp"

                android:layout_weight="1"
                android:background="@drawable/custom_button"
                android:gravity="center"
                android:text="@string/OrderReview"
                android:textColor="#ffffff"
                android:textSize="13sp"
                android:visibility="gone" />
        </LinearLayout>


        <android.inputmethodservice.KeyboardView
            android:id="@+id/keyboardview"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:visibility="gone"

            />

        <android.inputmethodservice.KeyboardView
            android:id="@+id/keyboardviewNum"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:visibility="gone" />

        <android.inputmethodservice.KeyboardView
            android:id="@+id/keyboardviewNumDecimal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:visibility="gone" />


    </LinearLayout>

</RelativeLayout>
